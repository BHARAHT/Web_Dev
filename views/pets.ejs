<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View Pets</title>
    <link rel="stylesheet" href="/Files/style.css">
</head>
<body>
    <h1>List All Pets</h1>
    <% for(pet of pets) { %>
    <div class="box">
    <p>Pet id : <%=pet.id%></p>
    <p>Pet Name : <%=pet.name%> </p>
    <p>Pet Species :<%=pet.species%> </p>
    <p>Pet Age : <%=pet.age%></p>
    <br>
    <a href="http://localhost:8000/pets/<%=pet.id%>">View</a>
    </div>

<%  } %>
<a href="/pets/new">Add Pets</a>
</body>
</html>
